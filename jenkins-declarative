pipeline{
    agent any
    
    environment{
        PATH= "/opt/maven3/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/javahometech/myweb'
            }
        }
        stage("Maven Build"){
            step{
                sh"mvn clean package"
            }
        }
    }
}
