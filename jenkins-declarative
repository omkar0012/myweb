pipeline{
    agent any
    
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/omkar0012/myweb'
            }
        }
        stage("Maven Build"){
            steps{
                def mvnHome = tool name: 'maven-3', type: 'maven'
                sh "${mvnHome}/bin/mvn package"
                
            }
        }
   }
}
